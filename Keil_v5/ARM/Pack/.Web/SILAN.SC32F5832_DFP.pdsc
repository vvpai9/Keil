	<keywords>
		<!-- keywords for indexing -->
		<keyword>SC32F58xx</keyword>
		<keyword>SC32F5832</keyword>
		<keyword>SC32F5864</keyword>
		<keyword>SC32F5832_BOOT</keyword>
		<keyword>SC32F5864_BOOT</keyword>
