  <conditions>
    <!-- Compiler Conditions -->
    <condition id="Compiler GCC">
      <require Tcompiler="GCC"/>
    </condition>
    <condition id="Compiler IAR">
      <require Tcompiler="IAR"/>
    </condition>

    <!-- Device Conditions -->
    <condition id="SIBG301">
      <description>Silicon Labs SIBG301 device series</description>
      <require Dvendor="Silicon Labs:21" Dname="SIBG301*"/>
      <require Cclass="CMSIS" Cgroup="CORE"/>
    </condition>
  </conditions>

  <components>
    <component Cclass="Device" Cgroup="Startup" Cversion="2025.6.2" condition="SIBG301">
      <description>System Startup for Silicon Labs SIBG301 device series</description>
      <files>
        <!-- include folder -->
        <file category="include" name="Device/SiliconLabs/SIBG301/Include/"/>
        <file category="header"  name="Device/SiliconLabs/SIBG301/Include/em_device.h"/>
        <!-- system file -->
        <file category="source"  name="Device/SiliconLabs/SIBG301/Source/system_sibg301.c" attr="config" version="2025.6.2"/>
      </files>
    </component>
