          </sequence>
          <sequence name='TraceStart'>
            <block>Sequence("TraceStart_CM4");</block>
          </sequence>
        </sequences>
        <memory name='EEPROM' start='0x40108000' size='0x4000' access='rx' default='1'/>
        <memory name='PROGRAM_FLASH' start='0x00000000' size='0x080000' access='rx' default='1' startup='1'/>
        <memory name='SRAMX' start='0x04000000' size='0x8000' access='rw' default='1'/>
        <memory name='SRAM_UPPER' start='0x20000000' size='0x028000' access='rw' default='1'/>
        <memory name='USB_RAM' start='0x40100000' size='0x2000' access='rw' default='1'/>
        <algorithm name='devices/LPC54618/arm/LPC5460x_512.FLM' start='0x00000000' size='0x00080000' RAMstart='0x20000000' RAMsize='0x00002000' default='1'/>
        <algorithm name='devices/LPC54618/arm/LPC5461x_MT25QL128.FLM' start='0x10000000' size='0x08000000' RAMstart='0x20000000' RAMsize='0x00002000' default='1'/>
        <debug svd='devices/LPC54618/LPC54618.xml'/>
        <variant Dvariant='LPC54618J512BD208'>
          <compile header='devices/LPC54618/fsl_device_registers.h' define='CPU_LPC54618J512BD208'/>
        </variant>
        <variant Dvariant='LPC54618J512ET180'>
