        <algorithm name="devices/MCXW236/arm/MCXW236.FLM" start="0x00000000" size="0x000FE000" RAMstart="0x20004000" RAMsize="0x00008000" default="1"/>
        <algorithm name="devices/MCXW236/arm/MCXW236_S.FLM" start="0x10000000" size="0x000FE000" RAMstart="0x20004000" RAMsize="0x00008000" default="1"/>
        <debug svd="devices/MCXW236/MCXW236.xml"/>
        <variant Dvariant="MCXW236BIHNAR">
