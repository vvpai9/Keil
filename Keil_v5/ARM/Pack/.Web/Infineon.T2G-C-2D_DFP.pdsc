      <algorithm Pname="Cortex-M7-0" name="Flash/CAT1C_SFLASH_TOC2.FLM" start="0x17007C00" size="0x00000200" RAMstart="0x28001000" RAMsize="0xFFF0" default="1" style="Keil" />
