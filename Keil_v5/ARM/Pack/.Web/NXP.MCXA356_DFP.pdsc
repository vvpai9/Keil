        <algorithm name="devices/MCXA356/arm/MCXA_1024.FLM" start="0x00000000" size="0xFE000" RAMstart="0x20000000" RAMsize="0x00001000" default="1"/>
        <debug svd="devices/MCXA356/MCXA356.xml"/>
        <variant Dvariant="MCXA356VLQ">
