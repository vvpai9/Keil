  <devices>
    <family Dfamily="MCXL254" Dvendor="NXP:11">
      <debugconfig default="swd" clock="5000000" swj="true"/>
      <sequences>
        <sequence name="EnableTraceClk">
          <block>
            __var SYSCON_NS_Base_Addr = 0x40091000;
            __var CGU_NS_Base_Addr    = 0xA0099000;
            __var TRACECLKSEL_Addr    = SYSCON_NS_Base_Addr + 0xD8;
            __var TRACECLKDIV_Addr    = SYSCON_NS_Base_Addr + 0xDC;
            __var CGU_PER_CLK_EN_Addr = CGU_NS_Base_Addr + 0xC;
            __var clksel              = 0;
            __var clkdiv              = 0;
          </block>
          <control if="__traceout &amp; 0x3">
            <block>clksel = Read32(TRACECLKSEL_Addr);  // Read current TRACECLKSEL value</block>
            <control if="clksel &gt; 2" info="Trace (TPIU) Clock not selected?">
              <block>Write32(TRACECLKSEL_Addr, 0x0);   // Select Trace divided clock</block>
            </control>
            <block>
              clkdiv = (Read32(TRACECLKDIV_Addr) &amp; 0xF);  // Read current TRACECLKDIV value, preserve divider but clear rest to enable
              Write32(TRACECLKDIV_Addr, clkdiv);
              
              Write32(CGU_PER_CLK_EN_Addr, (1 &lt;&lt; 5));   // Enable AON PORT clock
            </block>
          </control>
        </sequence>
        <sequence name="EnableDebugMailbox">
          <block>
            __var status=0x55aa;
            Message(0, "MCXL25x connect script start");
            
            // Read APIDR
            __dp = 0;
            __ap = 1;
            status = ReadAP(0xFC);
            Message(0, "APIDR: 0x%08X", status);
            
            // Read DPIDR
            __dp = 0;
            __ap = 1;
            status = ReadDP(0x0);
            Message(0, "DPIDR: 0x%08X", status);
            
            // Active DebugMailbox
            __dp = 0;
            __ap = 1;
            WriteAP(0x0, 0x00000021);
            DAP_Delay(1000000);
            ReadAP(0x0);
            
            // Enter Debug Session
            __dp = 0;
            __ap = 1;
            WriteAP(0x4, 0x00000007);
            DAP_Delay(30000);
            ReadAP(0x8);
            
            __dp = 0;
            __ap = 0;
            
            Message(0, "MCXL25x connect script end");
          </block>
        </sequence>
        <sequence name="DebugPortStart">
          <block>
            __var SW_DP_ABORT  = 0x0;
            __var DP_CTRL_STAT = 0x4;
            __var DP_SELECT    = 0x8;
            __var powered_down = 0;
            // Switch to DP Register Bank 0
            WriteDP(DP_SELECT, 0x00000000);
            
            // Read DP CTRL/STAT Register and check if CSYSPWRUPACK and CDBGPWRUPACK bits are set
            powered_down = ((ReadDP(DP_CTRL_STAT) &amp; 0xA0000000) != 0xA0000000);
          </block>
          <control if="powered_down">
            <block>
              // Request Debug/System Power-Up
              WriteDP(DP_CTRL_STAT, 0x50000000);
            </block>
            <!-- Wait for Power-Up Request to be acknowledged -->
            <control while="(ReadDP(DP_CTRL_STAT) &amp; 0xA0000000) != 0xA0000000" timeout="1000000"/>
            <control if="(__protocol &amp; 0xFFFF) == 1">
              <block>
                // Init AP Transfer Mode, Transaction Counter, and Lane Mask (Normal Transfer Mode, Include all Byte Lanes)
                // Additionally clear STICKYORUN, STICKYCMP, and STICKYERR bits by writing '1'
                WriteDP(DP_CTRL_STAT, 0x50000F32);
              </block>
            </control>
            <!-- SWD Specific Part of sequence -->
            <control if="(__protocol &amp; 0xFFFF) == 2">
              <block>
                // Init AP Transfer Mode, Transaction Counter, and Lane Mask (Normal Transfer Mode, Include all Byte Lanes)
                WriteDP(DP_CTRL_STAT, 0x50000F00);
                
                // Clear WDATAERR, STICKYORUN, STICKYCMP, and STICKYERR bits of CTRL/STAT Register by write to ABORT register
                WriteDP(SW_DP_ABORT, 0x0000001E);
              </block>
            </control>
            <block>
              __var ap0_disable=0x55aa;
              // Read AP0 CSW
              __dp = 0;
              __ap = 0;
              ap0_disable = ReadAP(0x0);
              ap0_disable = ((ap0_disable &amp; 0x40) == 0);
            </block>
            <control if="ap0_disable">
              <block>Sequence("EnableDebugMailbox");</block>
            </control>
          </control>
        </sequence>
        <sequence name="ResetCatchSetCore1">
          <block>
            // Use DEMCR.VC_CORERESET for CPU1
            __var SCS_Addr   = 0xE000E000;
            __var DHCSR_Addr = SCS_Addr + 0xDF0;
            __var DEMCR_Addr = SCS_Addr + 0xDFC;
            __var value      = 0;
            
            // Enable Reset Vector Catch in DEMCR
            value = Read32(DEMCR_Addr);
            Write32(DEMCR_Addr, (value | 0x00000001));
            // Read DHCSR to clear potentially set DHCSR.S_RESET_ST bit
            Read32(DHCSR_Addr);
          </block>
        </sequence>
        <sequence name="ResetCatchClear">
          <block>
            __var DEMCR_Addr = 0xE000EDFC;
            
            // Clear FPB Comparators 0 and 1
            Write32(0xE0002008, 0x00000000);
            
            // Disable FPB
            Write32(0xE0002000, 0x00000002);
            
            // Clear Reset Vector Catch
            Write32(DEMCR_Addr, Read32(DEMCR_Addr) &amp; (~0x00000001));
          </block>
          <block>
            // WriteEN index2
            Write32(0x40091D00, 0x00060000);
            Write32(0x40091D00, 0x00020002);
            Write32(0x40091D00, 0x00010002);
            Write32(0x40091D04, 0x00290000);
            Write32(0x40091D00, 0x00020002);
            Write32(0x40091D04, 0x00280000);
            Write32(0x40091D00, 0x00000002);
            
            // SRAM_XEN=0x3FF
            Write32(0x40091E58, 0x000003FF);
            Write32(0x40091E5C, 0x000003FF);
            
            // Update MEM0_BLK_CFG_W
            // WriteEN index15
            Write32(0x40091D00, 0x00060000);
            Write32(0x40091D00, 0x0002000F);
            Write32(0x40091D00, 0x0001000F);
            Write32(0x40091D04, 0x00290000);
            Write32(0x40091D00, 0x0002000F);
            Write32(0x40091D04, 0x00280000);
            Write32(0x40091D00, 0x0000000F);
            
            //MBC0_MEMN_GLBAC0=0x00007777, RWX
            Write32(0x4008E020, 0x00007777);
            
            //For Flash RWX
            //MBC0_DOM0_MEM0_BLK_CFG_W0..7=0x00000000
            Write32(0x4008E040, 0x00000000);
            Write32(0x4008E044, 0x00000000);
            Write32(0x4008E048, 0x00000000);
            Write32(0x4008E04C, 0x00000000);
            Write32(0x4008E050, 0x00000000);
            Write32(0x4008E054, 0x00000000);
            Write32(0x4008E058, 0x00000000);
            Write32(0x4008E05C, 0x00000000);
            
            //For IFR0 RWX
            //MBC0_DOM_MEM1_BLK_CFG_W0=0x00000000
            Write32(0x4008E180, 0x00000000);
          </block>
        </sequence>
        <sequence name="WaitForStopAfterReset">
          <block>
            __var SCS_Addr    = 0xE000E000;
            __var DHCSR_Addr  = SCS_Addr + 0xDF0;
            __var DFSR_Addr  = SCS_Addr + 0xD30;
            __var ap0_disable=0x55aa;
            
            // Give bootloader time to do what it needs to do
            DAP_Delay(BootTime);  // try 10ms
            
            //// __dp = 0;
            //// __ap = 1;
            //// WriteAP(0x0, 0x20);
            
            // Read AP0 CSW
            __dp = 0;
            __ap = 0;
            ap0_disable = ReadAP(0x0);
            ap0_disable = ((ap0_disable &amp; 0x40) == 0);
            Message(0, "AP0 DIS: %x", ap0_disable);
          </block>
          <!-- Reset Recovery: Wait for DHCSR.S_RESET_ST bit to clear on read -->
          <control if="ap0_disable">
            <block>Sequence("EnableDebugMailbox");</block>
          </control>
          <control while="(Read32(DHCSR_Addr) &amp; 0x02000000)" timeout="500000"/>
          <control if="(Read32(DHCSR_Addr) &amp; 0x00020000) == 0">
            <block>
              Write32(DHCSR_Addr, 0xA05F0003);  // Force halt until finding a proper catch. Probably works better from flash.
              // Clear watch point
              Write32(0xE0001028, 0x0);
              Write32(0xE0001038, 0x0);
              Write32(0xE0001048, 0x0);
              Write32(0xE0001058, 0x0);
            </block>
          </control>
        </sequence>
        <sequence name="ResetHardware">
          <block>
            __var nReset      = 0x80;
            __var canReadPins = 0;
            __var SCS_Addr    = 0xE000E000;
            __var DHCSR_Addr  = SCS_Addr + 0xDF0;
            __var DEMCR_Addr  = SCS_Addr + 0xDFC;
            __var tmp;
            
            //Halt the core
            Write32(DHCSR_Addr, 0xA05F0003);
            //clear VECTOR CATCH and set TRCENA
            tmp = Read32(DEMCR_Addr);
            Write32(DEMCR_Addr, tmp | 0x1000000);
            
            __errorcontrol = 1;
            
            // Set watch point
            Write32(0xE0001020, 0x00000000);
            Write32(0xE0001028, 0x412);
            Write32(0xE0001030, 0x00FFFFFF);
            Write32(0xE0001038, 0x403);
            
            // De-assert nRESET line
            canReadPins = (DAP_SWJ_Pins(0x00, nReset, 0) != 0xFFFFFFFF);
          </block>
          <!-- Keep reset active for 50 ms -->
          <control while="1" timeout="50000"/>
          <control if="canReadPins">
            <!-- Assert nRESET line and wait max. 1s for recovery -->
            <control while="(DAP_SWJ_Pins(nReset, nReset, 0) &amp; nReset) == 0" timeout="1000000"/>
          </control>
          <control if="!canReadPins">
            <block>
        // Assert nRESET line
        DAP_SWJ_Pins(nReset, nReset, 0);
      </block>
            <!-- Wait 100ms for recovery if nRESET not readable -->
            <control while="1" timeout="100000"/>
          </control>
          <control if="(__connection &amp; (1 &lt;&lt; 17)) == 0">
            <block>Sequence("WaitForStopAfterReset");</block>
          </control>
        </sequence>
        <sequence name="ResetSystemCore0">
          <block>
            // System Control Space (SCS) offset as defined in Armv6-M/Armv7-M.
            __var SCS_Addr   = 0xE000E000;
            __var AIRCR_Addr = SCS_Addr + 0xD0C;
            __var DHCSR_Addr = SCS_Addr + 0xDF0;
            __var DEMCR_Addr = SCS_Addr + 0xDFC;
            __var tmp;
            
            __var ap0_disable=0x55aa;
            
            //Halt the core
            Write32(DHCSR_Addr, 0xA05F0003);
            //clear VECTOR CATCH and set TRCENA
            tmp = Read32(DEMCR_Addr);
            Write32(DEMCR_Addr, tmp | 0x1000000);
            
            __errorcontrol = 1;
            
            // Set watch point
            Write32(0xE0001020, 0x00000000);
            Write32(0xE0001028, 0x412);
            Write32(0xE0001030, 0x00FFFFFF);
            Write32(0xE0001038, 0x403);
            
            __errorcontrol = 1;
            
            // Execute SYSRESETREQ via AIRCR
            Write32(AIRCR_Addr, 0x05FA0004);
            DAP_Delay(BootTime);  // try 10ms
            
            __errorcontrol = 0;
            
            // Give bootloader time to do what it needs to do
            DAP_Delay(BootTime);  // try 10ms
            Sequence("WaitForStopAfterReset");
          </block>
        </sequence>
        <sequence name="ResetProcessorCore1">
          <block>
            __var SYSCON_NS_Base_Addr = 0x40000000;
            __var CPUCTRL_Addr        = SYSCON_NS_Base_Addr + 0x800;
            __var value               = 0;
            __var apOrg               = __ap;
            
            __ap = 0;                           // Switch to CPU0 AP to drive CPU1 reset via CPUCTRL register
            
            // TODO: Reset CM0 only if kept in reset state
            
            // Set trap
            Write32(0xA1000000, 0x1000);        // Set valid core1 stack pointer
            Write32(0xA1000004, 0x101);         // Set the reset vector to the address 0x100, bit 0 set to 1 tells the CPU to expect thumb (2B log) instruction
            Write32(0xA1000100, 0xE7feE7fe);    // Set two thumb instructions jumping back to itself
            
            // Launch CM0
            Write32(0xA009900C, 0x7fe7);        // Enable all the clocks. (CGU-&gt;PER_CLK_EN register)
            DAP_Delay(300000);
            value = Read32(0xa0099010);         // Read RST_SUB_BLK Register
            value |= 0x0010;
            Write32(0xA0099010, value);         // Releases cm0 from reset by writing 1 to CMOP_RST_REL bit of CGU-&gt;RST_SUB_BLK register
            __ap = apOrg;
          </block>
        </sequence>
        <sequence name="TraceStart">
          <control if="__traceout &amp; 0x1" info="SWO Trace output selected">
            <block>
              Sequence("EnableTraceClk");       // Enable and configure trace clock
              Write32(0xA00850C0, 0x00001140);  // Configure PORT0_PCR16 mux to SWO
            </block>
          </control>
        </sequence>
      </sequences>
      <debugvars configfile="devices/MCXL254/arm/MCXL254.dbgconf">
        // Debug Access Variables, can be modified by user via copies of DBGCONF files as created by uVision. Also see sub-family level.
        __var Dbg_CR                = 0x00000000;            // DBG_CR
        __var BootTime              = 300000;                // 300 milliseconds
      </debugvars>
      <description>MCXL254</description>
      <device Dname="MCXL254">
        <processor Pname="cm33" Dcore="Cortex-M33" Dfpu="SP_FPU" Dmpu="MPU" Dtz="TZ" Ddsp="DSP" Dendian="Little-endian" Dclock="96000000"/>
        <processor Pname="cm0plus" Dcore="Cortex-M0+" Dfpu="NO_FPU" Dmpu="NO_MPU" Dtz="NO_TZ" Ddsp="NO_DSP" Dendian="Little-endian" Dclock="10000000"/>
        <environment Pname="cm33" name="iar">
          <file category="linkerfile" name="devices/MCXL254/iar/MCXL254_cm33_flash.icf"/>
        </environment>
        <environment Pname="cm0plus" name="iar">
          <file category="linkerfile" name="devices/MCXL254/iar/MCXL254_cm0plus_ram.icf"/>
        </environment>
        <sequences>
          <sequence name="ResetSystem" Pname="cm33">
            <block>Sequence("ResetSystemCore0");</block>
          </sequence>
          <sequence name="ResetSystem" Pname="cm0plus" disable="1"/>
          <sequence name="ResetProcessor" Pname="cm33" disable="1"/>
          <sequence name="ResetProcessor" Pname="cm0plus">
            <block>Sequence("ResetProcessorCore1");</block>
          </sequence>
        </sequences>
        <algorithm name="devices/MCXL254/arm/MCXL25X.FLM" start="0x00000000" size="0x00040000" RAMstart="0x28000000" RAMsize="0x00002000" default="1"/>
        <debug svd="devices/MCXL254/MCXL254_cm33.xml" Pname="cm33" __dp="0" __ap="0"/>
        <debug svd="devices/MCXL254/MCXL254_cm0plus.xml" Pname="cm0plus" __dp="0" __ap="2"/>
        <variant Dvariant="MCXL254VLL">
          <compile Pname="cm33" header="devices/MCXL254/fsl_device_registers.h" define="CPU_MCXL254VLL_cm33"/>
          <compile Pname="cm0plus" header="devices/MCXL254/fsl_device_registers.h" define="CPU_MCXL254VLL_cm0plus"/>
        </variant>
        <variant Dvariant="MCXL254VDF">
          <compile Pname="cm33" header="devices/MCXL254/fsl_device_registers.h" define="CPU_MCXL254VDF_cm33"/>
          <compile Pname="cm0plus" header="devices/MCXL254/fsl_device_registers.h" define="CPU_MCXL254VDF_cm0plus"/>
        </variant>
      </device>
    </family>
  </devices>
