        <memory name='SRAMX1' start='0x04002000' size='0x2000' access='rw' default='1'/>
        <algorithm name='devices/MCXA276/arm/MCXA27X_1024.FLM' start='0x00000000' size='0xFE000' RAMstart='0x20000000' RAMsize='0x00001000' default='1'/>
        <debug svd='devices/MCXA276/MCXA276.xml'/>
        <variant Dvariant='MCXA276VLQ'>
          <compile header='devices/MCXA276/fsl_device_registers.h' define='CPU_MCXA276VLQ'/>
        </variant>
        <variant Dvariant='MCXA276VLL'>
