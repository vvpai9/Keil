  <xs:element name="DebugOption">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Simulator">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="UseSimulator" type="switcher" />
              <xs:element name="LoadApplicationAtStartup" type="switcher" />
              <xs:element name="RunToMain" type="switcher" />
              <xs:element name="RestoreBreakpoints" type="switcher" />
              <xs:element name="RestoreWatchpoints" type="switcher" />
              <xs:element name="RestoreMemoryDisplay" type="switcher" />
