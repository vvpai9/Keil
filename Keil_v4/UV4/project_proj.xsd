<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="Project">
    <xs:complexType>      
      <xs:sequence>
        <xs:element name="SchemaVersion" type="xs:string" />
        <!--Header string-->
        <xs:element name="Header" type="xs:string" />
        <xs:element minOccurs="1" maxOccurs="1" name ="Targets">
          <xs:complexType>
            <xs:sequence>
              <xs:element minOccurs="1" maxOccurs="unbounded" name="Target"> <!--specification of target options-->
                <xs:complexType>
                  <xs:sequence> <!--at least one target each project-->
                    <xs:element name="TargetName" type="xs:string"/>
                    <xs:element name="ToolsetNumber" type="HexOrLong"></xs:element>
                    <xs:element name="ToolsetName" type="xs:string"></xs:element>
                    <xs:element minOccurs="0" maxOccurs="1" name="TargetOption"> <!--definition of target options-->
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element ref="TargetCommonOption"/>
                          <xs:element ref="CommonProperty"/>
                          <xs:element ref="DllOption"/>
                          <xs:element ref="DebugOption"/>
                          <xs:element ref="Utilities"/>
                          <xs:choice>
                            <xs:element name="Target166">
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element ref="Target166Misc"></xs:element>
                                  <xs:element ref="C166"></xs:element>
                                  <xs:element ref="Ec166"></xs:element>
                                  <xs:element ref="A166"></xs:element>
                                  <xs:element ref="L166"></xs:element>
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                            <xs:element name="Target51">
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element ref="Target51Misc"></xs:element>
                                  <xs:choice>
                                    <xs:element ref="C251"></xs:element>
                                    <xs:element ref="C51"></xs:element>
                                  </xs:choice>
                                  <xs:element ref="Ax51"></xs:element>
                                  <xs:element ref="Lx51"></xs:element>
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                            <xs:element name="Target251">
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element ref="Target251Misc"></xs:element>
                                  <xs:element ref="C251"></xs:element>
                                  <xs:element ref="Ax51"></xs:element>
                                  <xs:element ref="Lx51"></xs:element>
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                            <xs:element name="TargetArm">
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element ref="ArmMisc"></xs:element>
                                  <xs:element ref="Carm"></xs:element>
                                  <xs:element ref="Aarm"></xs:element>
                                  <xs:element ref="LDarm"></xs:element>
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                            <xs:element name="TargetArmAds">
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element ref="ArmAdsMisc"></xs:element>
                                  <xs:element ref="Cads"></xs:element>
                                  <xs:element ref="Aads"></xs:element>
                                  <xs:element ref="LDads"></xs:element>
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                          </xs:choice>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element minOccurs="0" maxOccurs="1" name="Groups">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element minOccurs="1" maxOccurs="unbounded" name="Group"> <!--1 or more groups contained in the target-->
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="GroupName" type="xs:string"/>
                                <xs:element minOccurs="0" maxOccurs="1" name="GroupOption">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element ref="CommonProperty" />
                                      <xs:choice>
                                        <xs:element name="Group51">
                                          <xs:complexType>
                                            <xs:sequence>
                                              <xs:choice>
                                                <xs:element ref="C251" />
                                                <xs:element ref="C51" />
                                              </xs:choice>
                                              <xs:element ref="Ax51" />
                                            </xs:sequence>
                                          </xs:complexType>
                                        </xs:element>
                                        <xs:element name="Group251">
                                          <xs:complexType>
                                            <xs:sequence>
                                              <xs:element ref="C251" />
                                              <xs:element ref="Ax51" />
                                            </xs:sequence>
                                          </xs:complexType>
                                        </xs:element>
                                        <xs:element name="Group166">
                                          <xs:complexType>
                                            <xs:sequence>
                                              <xs:element ref="C166" />
                                              <xs:element ref="Ec166" />
                                              <xs:element ref="A166" />
                                            </xs:sequence>
                                          </xs:complexType>
                                        </xs:element>
                                        <xs:element name="GroupArm">
                                          <xs:complexType>
                                            <xs:sequence>
                                              <xs:element ref="Carm" />
                                              <xs:element ref="Aarm" />
                                            </xs:sequence>
                                          </xs:complexType>
                                        </xs:element>
                                        <xs:element name="GroupArmAds">
                                          <xs:complexType>
                                            <xs:sequence>
                                              <xs:element ref="Cads" />
                                              <xs:element ref="Aads" />
                                            </xs:sequence>
                                          </xs:complexType>
                                        </xs:element>
                                      </xs:choice>
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>
                                <xs:element name="Files" minOccurs="0" maxOccurs="1">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element minOccurs="1" maxOccurs="unbounded" name="File">
                                        <xs:complexType>
                                          <xs:sequence>
                                            <xs:element name="FileName" type="xs:string"/>
                                            <xs:element name="FileType" type="xs:byte"/>
                                            <xs:element name="FilePath" type="xs:string"/>
                                            <xs:element minOccurs="0" maxOccurs="unbounded" name="FileOption">
                                              <xs:complexType>
                                                <xs:sequence>
                                                  <xs:element ref="CommonProperty" />
                                                  <xs:choice>
                                                    <xs:element name="File51">
                                                      <xs:complexType>
                                                        <xs:choice>
                                                          <xs:element minOccurs="0" maxOccurs="1" ref="C251" />
                                                          <xs:element minOccurs="0" maxOccurs="1" ref="C51" />
                                                          <xs:element minOccurs="0" maxOccurs="1" ref="Ax51" />
                                                        </xs:choice>
                                                      </xs:complexType>
                                                    </xs:element>
                                                    <xs:element name="File251">
                                                      <xs:complexType>
                                                        <xs:choice>
                                                          <xs:element minOccurs="0" maxOccurs="1" ref="C251" />
                                                          <xs:element minOccurs="0" maxOccurs="1" ref="Ax51" />
                                                        </xs:choice>
                                                      </xs:complexType>
                                                    </xs:element>
                                                    <xs:element name="File166">
                                                      <xs:complexType>
                                                        <xs:choice>
                                                          <xs:element minOccurs="0" maxOccurs="1" ref="C166" />
                                                          <xs:element minOccurs="0" maxOccurs="1" ref="Ec166" />
                                                          <xs:element minOccurs="0" maxOccurs="1" ref="A166" />
                                                        </xs:choice>
                                                      </xs:complexType>
                                                    </xs:element>
                                                    <xs:element name="FileArm">
                                                      <xs:complexType>
                                                        <xs:choice>
                                                          <xs:element minOccurs="0" maxOccurs="1" ref="Carm" />
                                                          <xs:element minOccurs="0" maxOccurs="1" ref="Aarm" />
                                                        </xs:choice>
                                                      </xs:complexType>
                                                    </xs:element>
                                                    <xs:element name="FileArmAds">
                                                      <xs:complexType>
                                                        <xs:choice>
                                                          <xs:element minOccurs="0" maxOccurs="1" ref="Cads" />
                                                          <xs:element minOccurs="0" maxOccurs="1" ref="Aads" />
                                                        </xs:choice>
                                                      </xs:complexType>
                                                    </xs:element>
                                                  </xs:choice>
                                                </xs:sequence>
                                              </xs:complexType>
                                            </xs:element>
                                          </xs:sequence>
                                        </xs:complexType>
                                      </xs:element>
                                    </xs:sequence>
                                  </xs:complexType>
                              </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <!--common options specific to each target-->
