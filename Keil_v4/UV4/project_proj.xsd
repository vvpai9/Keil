        <xs:element name="DebugInformation" type="switcher" />
        <xs:element name="BrowseInformation" type="switcher" />
        <xs:element name="ListingPath" type="xs:string" />
        <xs:element name="HexFormatSelection" type="xs:unsignedByte" />
        <xs:element name="Merge32K" type="switcher" />
        
        <xs:element name="CreateBatchFile" type="switcher" />
        <xs:element name="BeforeCompile" type="UserProgSettings"/>
        <xs:element name="BeforeMake" type="UserProgSettings"/>
