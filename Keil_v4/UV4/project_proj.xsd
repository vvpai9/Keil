              <xs:element name="LimitSpeedToRealTime" type="switcher" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="Target">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="UseTarget" type="switcher" />
              <xs:element name="LoadApplicationAtStartup" type="switcher" />
              <xs:element name="RunToMain" type="switcher" />
              <xs:element name="RestoreBreakpoints" type="switcher" />
